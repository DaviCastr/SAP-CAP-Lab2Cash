{
  "name": "lab2cash",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/connectivity": "^3",
    "@sap-cloud-sdk/http-client": "^4.0.2",
    "@sap-cloud-sdk/resilience": "^4.0.2",
    "@sap/cds": "^7",
    "@sap/xssec": "^3",
    "@sap/cds-hana": "^2",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "@sap/dev-cap-tools": "^1.31.0"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "[production]": {
        "auth": "xsuaa",
        "db": "hana"
      },
      "LAB2CASH_PROXY": {
        "kind": "odata",
        "model": "srv/external/LAB2CASH_PROXY",
        "credentials": {
          "destination": "LAB2CASH_PROXY",
          "path": "/proxy"
        }
      }
    },
    "sql": {
      "native_hana_associations": false
    }
  }
}